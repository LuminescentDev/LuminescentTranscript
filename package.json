{
  "name": "luminescent-transcripts",
  "version": "3.2.0",
  "description": "A transcript service for Discord",
  "repository": {
    "type": "git",
    "url": "https://github.com/LuminescentDev/LuminescentTranscript"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview",
    "start": "vite --mode ssr",
    "start:vite": "vite --mode ssr",
    "start:vite.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --forcenode --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.6",
    "@builder.io/qwik-city": "1.2.6",
    "@types/eslint": "8.44.0",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.45.0",
    "eslint-plugin-qwik": "1.2.6",
    "path": "^0.12.7",
    "postcss": "^8.4.26",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^11.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "undici": "^5.22.1",
    "unified": "^11.0.0",
    "vite": "4.4.4",
    "vite-tsconfig-paths": "4.2.0",
    "zod": "^3.21.4"
  }
}
